DROP TABLE IF EXISTS `ls_favorite`;
CREATE TABLE `ls_favorite` (
  `id` varchar(32) NOT NULL COMMENT '主键',
  `prod_id` int(11) NOT NULL DEFAULT '0' COMMENT '商品ID',
  `prod_name` varchar(120) NOT NULL DEFAULT '' COMMENT '收藏的商品名称',
