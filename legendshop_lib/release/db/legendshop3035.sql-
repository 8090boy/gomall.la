DROP TABLE IF EXISTS `ls_brand`;
CREATE TABLE `ls_brand` (
  `brand_id` int(11) NOT NULL AUTO_INCREMENT COMMENT '主键',
  `brand_name` varchar(30) NOT NULL DEFAULT '' COMMENT '品牌名称',
