<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- 
 * 
 * LegendShop 多用户商城系统
 * 
 *  版权所有,并保留所有权利。
 * 
  -->
<hibernate-mapping>
	<class name="com.legendshop.model.entity.Area" table="ls_areas">
		<id name="id"  type="java.lang.Integer">
			<column name="id" />
			<generator class="native" />
		</id>
    	
    		<property name="areaid" column="areaid" type="java.lang.String" />
    	
    		<property name="area" column="area" type="java.lang.String" />
    	
    		<property name="cityid" column="cityid" type="java.lang.Integer" />
    		
    		<property name="code" column="code" type="java.lang.Integer" />
	</class>
</hibernate-mapping>
