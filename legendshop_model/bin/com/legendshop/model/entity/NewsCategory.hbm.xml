<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!--  LegendShop 版权所有 2009-2011,并保留所有权利。 -->
<hibernate-mapping>
	<class name="com.legendshop.model.entity.NewsCategory" table="ls_news_cat">
	<id name="newsCategoryId" column="news_category_id" type="java.lang.Long">
     <generator class="native" />
	</id>
	<property name="newsCategoryName" column="news_category_name" type="java.lang.String" />
	<property name="status" column="status" type="java.lang.Short" />
	<property name="newsCategoryDate" column="news_category_date" type="java.util.Date" />
	<property name="userId" column="user_id" type="java.lang.String" />
	<property name="userName" column="user_name" type="java.lang.String" />
	<property name="seq" type="java.lang.Integer">
            <column name="seq">
                <comment>顺序</comment>
            </column>
       </property>
	</class>
</hibernate-mapping>
