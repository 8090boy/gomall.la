<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!--
	@LegendShop 商城系统. Create in 2012-07-15 15:12:59
-->
<hibernate-mapping>
	<class name="com.legendshop.model.entity.Tag" table="ls_tag">
	<id name="tagId" column="tag_id" type="java.lang.Long">
     <generator class="native" />
	</id>
	<property name="name" column="name" type="java.lang.String" />
	<property name="sortId" column="sort_id" type="java.lang.Long" />
	<property name="newsCategoryId" column="news_category_id" type="java.lang.Long" />
	<property name="type" column="type" type="java.lang.String" />
	<property name="status" column="status" type="java.lang.Integer" />
	<property name="createTime" column="create_time" type="java.util.Date" />
	<property name="userId" column="user_id" type="java.lang.String" />
	<property name="userName" column="user_name" type="java.lang.String" />

	</class>
</hibernate-mapping>
